		"bcrypt": "^3.0.5",
		"connect-redis": "^3.4.1",
		"dotenv": "^7.0.0",
		"express": "^4.16.4",
		"express-session": "^1.16.1",
		"graphql": "^14.2.1",
		"graphql-voyager": "^1.0.0-rc.26",
		"http": "^0.0.0",
		"joi": "^14.3.1",
		"jsonwebtoken": "^9.0.0",
		"mongoose": "^5.5.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.4.3",
		"@babel/core": "^7.4.3",
		"@babel/node": "^7.2.2",
		"@babel/plugin-transform-runtime": "^7.4.3",
		"@babel/preset-env": "^7.4.3",
		"apollo": "^2.8.3",
